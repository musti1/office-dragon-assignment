{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": {
          "version": 3,
          "proposals": true
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": "css"
      },
      "ant"
    ],
    ["@babel/plugin-proposal-class-properties"],
    "react-hot-loader/babel",
    "@babel/plugin-syntax-dynamic-import"
  ]
}
